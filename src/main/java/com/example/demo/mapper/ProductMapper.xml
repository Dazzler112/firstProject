<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ProductMapper">
	<select id="selectAll2"
		resultType="com.example.demo.domain.Product">
		SELECT * FROM Product ORDER BY ID DESC
	</select>
	<select id="selectAll3"
		resultType="com.example.demo.domain.Product">
		SELECT * FROM Product ORDER BY Likes DESC
	</select>
	<select id="selectAll4"
		resultType="com.example.demo.domain.Product">
		SELECT p.*
		FROM Product p
		JOIN ProductLike pl ON p.id = pl.productId
		WHERE pl.memberId = #{memberId}
	</select>
	<select id="selectAll5"
		resultType="com.example.demo.domain.Product">
		SELECT * FROM Product
	</select>
	<select id="selectAll6"
		resultType="com.example.demo.domain.Product">
		SELECT * FROM Product
	</select>
	<select id="selectAll7"
		resultType="com.example.demo.domain.Product">
		SELECT * FROM Product ORDER BY ID DESC
	</select>
	
</mapper>